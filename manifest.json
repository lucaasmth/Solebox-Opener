{
	"manifest_version": 2,
	"name": "Solebox Region Opener",
	"version": "0.2",
	"description": "Open Solebox product page to your region using PID or SKU",
	"icons": {
		"16": "images/favicon-16x16.png",
		"32": "images/favicon-32x32.png",
		"96": "images/favicon-96x96.png",
		"128": "images/favicon-128x128.png"
	},
	"browser_action": {
		"default_popup": "popup.html"
	},
	"author": "lucaasmth",
	"permissions": [
		"storage"
	]
}